#include <stdio.h>

int main()
{
    int num;
    printf("enter the number");
    scanf("%d",&num);
    return 0;
}
  int fact;
  int factorial(int num)
  {
     if(num==1)
     {
        return 1;
     }
    else
    {
        fact=num*factorial(num-1);
        return fact;
    }
    
  }
    
